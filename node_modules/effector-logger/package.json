{
  "name": "effector-logger",
  "version": "0.13.4",
  "main": "./index.js",
  "module": "./index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.mjs"
    },
    "./index.mjs": "./index.mjs",
    "./attach": {
      "import": "./attach.mjs",
      "require": "./attach.js",
      "default": "./attach.mjs"
    },
    "./inspector": {
      "import": "./inspector.mjs",
      "require": "./inspector.js",
      "default": "./inspector.mjs"
    },
    "./babel-plugin": {
      "default": "./babel-plugin.js"
    },
    "./macro": {
      "default": "./macro.js"
    }
  },
  "typesVersions": {
    "*": {
      "attach": [
        "dist/attach.d.ts"
      ]
    }
  },
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "repository": "github:effector/logger",
  "files": [
    "dist",
    "attach.js",
    "attach.js.map",
    "attach.mjs",
    "attach.mjs.map",
    "index.js",
    "index.js.map",
    "index.mjs",
    "index.mjs.map",
    "inspector.js",
    "inspector.js.map",
    "inspector.mjs",
    "inspector.mjs.map",
    "babel-plugin.js",
    "macro.js",
    "macro.d.ts"
  ],
  "scripts": {
    "start": "parcel serve ./usage/index.html",
    "build": "rm -rf ./dist && node --loader ts-node/esm ./scripts/build.ts",
    "build:integration": "LIB_NAME=@effector/logger yarn build",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "echo no tests yet",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "effector": "^22.1.0"
  },
  "dependencies": {
    "detect-node": "2.0.4",
    "effector-inspector": "^0.8.2",
    "just-debounce-it": "^1.1.0",
    "set-value": "4.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
